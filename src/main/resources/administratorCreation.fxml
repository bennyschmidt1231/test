<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
  prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121"
  xmlns:fx="http://javafx.com/fxml/1"
  fx:controller="seng302.controllers.CreateAdministratorPaneController">
  <children>
    <AnchorPane prefHeight="136.0" prefWidth="600.0">
      <children>
        <Label fx:id="createAccountLabel" layoutX="202.0" text="Create Administrator">
          <font>
            <Font size="25.0"/>
          </font>
        </Label>
        <Label fx:id="errorLabel" layoutX="32.0" layoutY="36.0" prefHeight="89.0" prefWidth="537.0"
          textFill="RED"/>
      </children>
    </AnchorPane>
    <AnchorPane prefHeight="281.0" prefWidth="600.0">
      <children>
        <Label layoutX="158.0" layoutY="14.0" text="Username">
          <font>
            <Font size="17.0"/>
          </font>
        </Label>
        <Label layoutX="143.0" layoutY="58.0" text="Given Name">
          <font>
            <Font size="17.0"/>
          </font>
        </Label>
        <Label layoutX="130.0" layoutY="100.0" text="Middle Name">
          <font>
            <Font size="17.0"/>
          </font>
        </Label>

        <Label layoutX="156.0" layoutY="145.0" text="Last Name">
          <font>
            <Font size="17.0"/>
          </font>
        </Label>
        <Label layoutX="160.0" layoutY="189.0" text="Password">
          <font>
            <Font size="17.0"/>
          </font>
        </Label>
        <Label layoutX="100.0" layoutY="232.0" text="Confirm Password">
          <font>
            <Font size="17.0"/>
          </font>
        </Label>
        <TextField fx:id="usernameTextField" layoutX="248.0" layoutY="14.0"/>
        <TextField fx:id="givenNameTextField" layoutX="248.0" layoutY="58.0"/>
        <TextField fx:id="middleNameTextField" layoutX="248.0" layoutY="100.0"/>
        <TextField fx:id="lastNameTextField" layoutX="248.0" layoutY="145.0"/>
        <PasswordField fx:id="initialPasswordField" layoutX="248.0" layoutY="189.0"/>
        <PasswordField fx:id="confirmPasswordField" layoutX="248.0" layoutY="232.0"/>
        <Button fx:id="doneButton" layoutX="508.0" layoutY="243.0" mnemonicParsing="false"
          onAction="#createAccount" text="done">
          <font>
            <Font size="15.0"/>
          </font>
        </Button>
        <Button fx:id="cancelButton" layoutX="426.0" layoutY="243.0" mnemonicParsing="false"
          onAction="#cancelSelected" text="cancel">
          <font>
            <Font size="15.0"/>
          </font>
        </Button>
      </children>
    </AnchorPane>
  </children>
</VBox>
