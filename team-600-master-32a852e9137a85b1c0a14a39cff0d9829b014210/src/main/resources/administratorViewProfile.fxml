<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="seng302.controllers.administratorViewProfileController">
   <children>
     <Label fx:id="errorLabel" layoutX="45.0" layoutY="30.0" prefHeight="59.0" prefWidth="527.0" textFill="RED" />
      <GridPane layoutX="-14.0" layoutY="-14.0" prefHeight="400.0" prefWidth="600.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="First Name: " GridPane.halignment="CENTER" GridPane.rowIndex="1" />
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Middle name(s): " GridPane.halignment="CENTER" GridPane.rowIndex="2" />
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Last Name: " GridPane.halignment="CENTER" GridPane.rowIndex="3" />
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Username: " GridPane.halignment="CENTER" GridPane.rowIndex="4" />
            <Label fx:id="adminFirstName" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Label fx:id="adminMiddleNames" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <Label fx:id="adminLastName" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <Label fx:id="adminUsername" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="4" />
            <Button fx:id="editButton" mnemonicParsing="false" onAction="#editClicked" text="Edit" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.valignment="CENTER" />
            <TextField fx:id="adminEditFirstName" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <TextField fx:id="adminEditMiddleNames" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <TextField fx:id="adminEditLastName" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <TextField fx:id="adminEditUserName" GridPane.columnIndex="1" GridPane.rowIndex="4" />
            <Button fx:id="doneButton" mnemonicParsing="false" onAction="#doneClicked" prefHeight="25.0" prefWidth="153.0" text="Done" GridPane.columnIndex="1" />
            <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#cancelClicked" prefHeight="25.0" prefWidth="148.0" text="Cancel" GridPane.columnIndex="1" GridPane.halignment="RIGHT" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
